{
  "name": "journey-planner",
  "version": "0.0.0",
  "description": "Keeping your peeps informed about where you're gonna be, who you're gonna be with, and how you're getting around. ",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --exit test",
    "start": "nodemon app.js",
    "lint": "jshint app.js src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swoloszynski/journey-planner.git"
  },
  "author": "Sam Woloszynski and Ben Travis",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/swoloszynski/journey-planner/issues"
  },
  "homepage": "https://github.com/swoloszynski/journey-planner#readme",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "mysql2": "1.6.1",
    "sequelize": "4.38.0",
    "yarn": "1.7.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "jshint": "2.9.6",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "sequelize-cli": "4.0.0",
    "supertest": "3.1.0"
  }
}
